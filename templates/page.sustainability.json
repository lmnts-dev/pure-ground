    "16608418622ecf558a": {
      "type": "icons-section",
      "settings": {
        "caption": "",
        "heading": "Our commitment to UN's Sustainable Development Goals",
        "subheading": "",
        "heading_size": "h4",
        "accent_color": false,
        "text": "<p>We believe in self responsibility and collective action. We are proud to have aligned with the UN’s integral approach and call to action to end poverty, protect the planet, and ensure that by 2030 all people enjoy peace and prosperity.<\/p><p>We know we can all work together to create a happier, healthier world.<\/p>",
        "button_label": "Button label",
        "button_link": "",
        "alt": "",
        "layout": "row",
        "icon_height": 210,
        "color_scheme": "cream",
        "top_spacing": 50,
